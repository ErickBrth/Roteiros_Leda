public class Quizz22 {

	int[] C = new int[A.length + B.length];
	int b = B.length - 1;
	int a = 0;
	int c = 0;
	while(b >= 0 && a < A.length) {
		if(b < 0) {
			C[c] = a[a];
			break;
		}
		else if (a > a.length) {
			C[c] = B[a];
			break;
		}
		if(B[b] >= a[a]) {
			C[c] = a[a];
			c++;
			a++;
		}
		else {
			C[c] = B[b];
			c++;
			b--;
		}
	}

	
return C;
}
}
